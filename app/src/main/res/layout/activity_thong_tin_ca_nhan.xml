<?xml version="1.0" encoding="utf-8"?><!-- Thông tin cá nhân -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/th_ng_tin_c"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true">

    <RelativeLayout
        android:id="@+id/angle_left"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="27dp"
        android:background="@drawable/angle_left"
        android:clipToOutline="true" />

    <TextView
        android:id="@+id/tai_khoan"
        android:layout_width="226dp"
        android:layout_height="29dp"
        android:layout_alignTop="@id/angle_left"
        android:layout_toRightOf="@id/angle_left"
        android:gravity="center_vertical"
        android:text="@string/tai_khoan"
        android:textAppearance="@style/n_h_ng_g222" />
    <!-- Rectangle 4186 -->
    <View
        android:id="@+id/rectangle_4186"
        android:layout_width="370dp"
        android:layout_height="170dp"
        android:layout_below="@+id/tai_khoan"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:background="@drawable/rectangle_4186" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/rectangle_4186"
        android:layout_alignTop="@+id/rectangle_4186"
        android:layout_marginStart="2dp"
        android:layout_marginTop="-2dp"
        app:srcCompat="@drawable/avatar" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/imageView"
        android:layout_alignBottom="@+id/imageView"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="35dp"
        app:srcCompat="@drawable/mini_camera" />
    <!-- Lê Trường Giang -->
    <TextView
        android:id="@+id/ho_ten"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/rectangle_4186"
        android:layout_alignEnd="@+id/rectangle_4186"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="193dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="8dp"
        android:layout_toEndOf="@+id/imageView"
        android:gravity="center_vertical"
        android:text="@string/ho_ten"
        android:textAppearance="@style/ho_ten" />

    <TextView
        android:id="@+id/email_giang"
        android:layout_width="170dp"
        android:layout_height="15dp"
        android:layout_below="@+id/ho_ten"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="22dp"
        android:layout_toEndOf="@+id/imageView"
        android:gravity="center_vertical"
        android:text="@string/email_giang"
        android:textAppearance="@style/email_giang" />
    <!-- Điện thoại: 08686868686 -->
    <TextView
        android:id="@+id/phone_number"
        android:layout_width="151dp"
        android:layout_height="15dp"
        android:layout_below="@+id/email_giang"
        android:layout_alignStart="@+id/email_giang"
        android:layout_alignParentLeft="false"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="199dp"
        android:layout_marginTop="12dp"
        android:gravity="center_vertical"
        android:text="@string/phone_number"
        android:textAppearance="@style/phone_number" />
    <!-- Vector -->
    <View
        android:id="@+id/vector_smail_pencil"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_alignEnd="@+id/rectangle_4186"
        android:layout_alignBottom="@+id/email_giang"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="false"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/vector_small_pencil" />

    <View
        android:id="@+id/vector_smail_pencil2"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_alignEnd="@+id/rectangle_4186"
        android:layout_alignBottom="@+id/phone_number"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="false"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/vector_small_pencil" />
    <!-- Địa chỉ: -->
    <TextView
        android:id="@+id/dia_chi"
        android:layout_width="71dp"
        android:layout_height="24dp"
        android:layout_below="@+id/rectangle_4186"
        android:layout_alignStart="@+id/rectangle_4186"
        android:layout_alignLeft="@id/rectangle_4186"
        android:layout_marginStart="0dp"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:text="Địa chỉ"
        android:textAppearance="@style/dia_chi" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_alignRight="@id/rectangle_4186"
        android:layout_alignBottom="@+id/dia_chi"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="333dp"
        android:layout_marginTop="265dp"
        android:layout_marginBottom="-1dp"
        android:background="@drawable/rectangle_4" />

    <TextView
        android:id="@+id/th_m"
        android:layout_width="34dp"
        android:layout_height="15dp"
        android:layout_alignStart="@+id/rectangle_4"
        android:layout_alignTop="@+id/rectangle_4"
        android:layout_alignParentLeft="false"
        android:layout_centerInParent="true"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="345dp"
        android:layout_marginTop="3dp"
        android:gravity="center_vertical"
        android:text="Thêm"
        android:textAppearance="@style/thay_doi" />
    <!-- 69 ngõ 697 Giải Phóng, Giáp Bát,Hoàng Mai, Hà Nội -->
    <TextView
        android:id="@+id/dia_chi_mac_dinh"
        android:layout_width="186dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/dia_chi"
        android:layout_alignLeft="@id/dia_chi"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:gravity="center_vertical"
        android:layout_marginTop="11dp"
        android:text="@string/ng_697_gi_i"
        android:textAppearance="@style/ng_697_gi_i" />

    <View
        android:id="@+id/rectangle_4_2"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_alignRight="@id/rectangle_4186"
        android:layout_alignTop="@+id/dia_chi_mac_dinh"
        android:background="@drawable/rectangle_4"
        android:layout_alignLeft="@id/rectangle_4"
        />
    <!-- Xóa -->
    <TextView
        android:id="@+id/x_a"
        android:layout_width="23dp"
        android:layout_height="15dp"
        android:layout_alignStart="@+id/rectangle_4_2"
        android:layout_alignTop="@+id/rectangle_4_2"

        android:layout_alignEnd="@+id/rectangle_4_2"
        android:layout_alignBottom="@+id/rectangle_4_2"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="3dp"
        android:layout_alignLeft="@id/th_m"
        android:text="Xóa"
        android:textAppearance="@style/thay_doi" />
    <!-- Rectangle 4196 -->

    <View
        android:id="@+id/rectangle_4_3"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_alignRight="@id/rectangle_4186"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="163dp"
        android:layout_marginTop="348dp"
        android:layout_marginRight="-3dp"
        android:background="@drawable/rectangle_4196" />

    <TextView
        android:id="@+id/txt_doi_mk"
        android:layout_width="127dp"
        android:layout_height="24dp"
        android:layout_alignStart="@+id/rectangle_4_3"

        android:layout_alignTop="@+id/rectangle_4_3"
        android:layout_centerInParent="true"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="2dp"
        android:gravity="center_vertical"
        android:text="Đổi mật khẩu"
        android:textAppearance="@style/txt_doi_mk" />

    <Button
        android:id="@+id/btnThemPTTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/rectangle_4_3"
        android:layout_alignBottom="@+id/rectangle_4_3"
        android:layout_marginStart="-108dp"
        android:layout_marginBottom="-95dp"
        android:text="Thêm phương thức thanh toán" />
</RelativeLayout>